{
    "name": "Scripting API Demo",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Uses the chrome.scripting API to inject JavaScript into web pages.",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["scripting", "webNavigation", "storage"],
    "host_permissions": ["*://*.yonyoucloud.com/*"],
    "action": {
        "default_title": "Default Title",
        "default_popup": "popup.html",
        "default_icon": {
            "32": "icon.png",
            "72": "icon.png",
            "128": "icon.png",
            "512": "icon.png"
        }
    },
    "icons": {
        "32": "icon.png",
        "72": "icon.png",
        "128": "icon.png",
        "512": "icon.png"
    }
}